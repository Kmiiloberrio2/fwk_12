<Project  ToolsVersion="3.5" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">  
    
	
    <PropertyGroup>
		<OutputPath_root>c:\projects\Entregas\CompilacionFwk\</OutputPath_root> 
		<SolutionPath_AnyCPU_debug>c:\projects\GitHub-sourcetree\fwk_12\Fwk\Fwk_AnyCPU_debug.sln</SolutionPath_AnyCPU_debug> 
		<OutputPath_AnyCPU_debug>Fwk_12_AnyCPU_debug</OutputPath_AnyCPU_debug>
		
		<SolutionPath_x64_debug>c:\projects\GitHub-sourcetree\fwk_12\Fwk\Fwk_x64_debug.sln</SolutionPath_x64_debug> 
		<OutputPath_x64_debug>Fwk_12_x64_debug</OutputPath_x64_debug>
		
		<SolutionPath_x64_release>c:\projects\GitHub-sourcetree\fwk_12\Fwk\Fwk_x64_release.sln</SolutionPath_x64_release> 
		<OutputPath_x64_release>Fwk_12_x64_release</OutputPath_x64_release>
		
	</PropertyGroup>  
	<Target Name="All"  DependsOnTargets="Compile_AnyCPU_debug;Compile_x64_debug;Compile_x64_release"></Target>  
	<Target Name="Compile_AnyCPU_debug" >
		<Message Text ="Realizando la compilacion AnyCPU_debug  del fwk" />
		<MSBuild Properties = "OutputPath=$(OutputPath_root)$(OutputPath_AnyCPU_debug)" Projects= "$(SolutionPath_AnyCPU_debug)" />
	</Target>  
    <Target Name="Compile_x64_debug" >
		<Message Text ="Realizando la compilacion Compile_x64_debug del fwk" />
		<MSBuild Properties = "OutputPath=$(OutputPath_root)$(OutputPath_x64_debug)" Projects= "$(SolutionPath_x64_debug)" />
	</Target>  
	
	 <Target Name="Compile_x64_release" >
		<Message Text ="Realizando la compilacion Compile_x64_release del fwk" />
		<MSBuild Properties = "OutputPath=$(OutputPath_root)$(OutputPath_x64_release)" Projects= "$(SolutionPath_x64_release)" />
	</Target> 
</Project> 